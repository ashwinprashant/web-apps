<%
contacts = Contact.all
%>

<h1>Contacts</h1>


<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Phone Number</th>
        <th>Company</th>
    </tr>
    <% for contact in contacts %>
        
    <tr>
        <th><%= contact["first_name"] %></th>
        <th><%= contact["last_name"] %></th>
        <th><%= contact["email"] %></th>
        <th><%= contact["phone_number"]%></th>
        <th><%company = Company.find_by({"id" => contact["company_id"]})%></th>%
        <th><%= company["name"]%></th>
    <% end %>
</table>